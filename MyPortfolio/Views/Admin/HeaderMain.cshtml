@model MainPersonelContextModel
@{
    ViewData["Title"] = "HeaderMain";
    Layout = "~/Views/Shared/_adminLayout.cshtml";
}


    <form asp-controller="Admin" asp-action="HeaderMain" method="post" enctype="multipart/form-data">
        <div class="text-danger" asp-validation-summary="All"></div>
       
        <div class="form-group">
            <label asp-for="PersonName"></label>
            <input asp-for="PersonName" class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="PersonWorkInstructure"></label>
            <input asp-for="PersonWorkInstructure" class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="Age"></label>
            <input asp-for="Age" class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="PhoneNumber"></label>
            <input asp-for="PhoneNumber" class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="Email"></label>
            <input asp-for="Email" class="form-control" />
        </div>
        <div class="form-group">
            <input type="file" name="file" id="file" class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="Adress"></label>
            <input asp-for="Adress" class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="HelloHeaderDescription"></label>
            <input asp-for="HelloHeaderDescription" class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="HelloSectionDescription"></label>
            <input asp-for="HelloSectionDescription" class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="ResumeHeaderDescription"></label>
            <input asp-for="ResumeHeaderDescription" class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="ResumeSectionDescription"></label>
            <input asp-for="ResumeSectionDescription" class="form-control" />
        </div>

        <div class="form-group">
            <label asp-for="GithubLink"></label>
            <input asp-for="GithubLink" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="LinkedikLink"></label>
            <input asp-for="LinkedikLink" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="Skype"></label>
            <input asp-for="Skype" class="form-control" />
        </div>

        <input type="submit" class="btn btn-primary" value="save" />

    </form>


@model GeneralSkillsModel
@{
    ViewData["Title"] = "GeneralSkills";
    Layout = "~/Views/Shared/_adminLayout.cshtml";
}


<form asp-controller="Admin" asp-action="Home" method="post">
    <div class="text-danger" asp-validation-summary="All"></div>

    <div class="form-group">
        <label asp-for="GeneralSkillsName"></label>
        <input asp-for="GeneralSkillsName" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="GeneralSkillsPercentage"></label>
        <div><span class="text-danger" asp-validation-for="GeneralSkillsPercentage"></span></div>
        <input asp-for="GeneralSkillsPercentage" class="form-control" />
    </div>
    <input type="submit" class="btn btn-primary" value="save" />

</form>

